{
    "name": "daskon/usersapicall",
    "version": "1.1.0",
    "description": "This wp plugin use to call external API called JSONPlaceholder",
    "type": "wordpress-plugin",
    "license": "GPL",
    "homepage": "https://github.com/daskon/usersapicall",
    "authors": [
        {
            "name": "Asela Daskon",
            "email": "admin@netlinkler.com",
            "homepage": "https://netlinkler.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Netlinkler\\UsersApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netlinkler\\UsersApi\\Tests\\Unit\\": "tests/PHPUnit/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9",
        "inpsyde/php-coding-standards": "^1@dev"
    }
}
